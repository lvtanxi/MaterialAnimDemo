<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ATLoginButton">
        <attr name="button_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="circle_loading_color" format="color" />
        <attr name="failed_button_color" format="color" />
        <attr name="text_size" format="dimension" />
        <attr name="circle_loading_width" format="dimension" />
        <attr name="login_text" format="string" />
        <attr name="failed_text" format="string" />
        <attr name="failed_text_color" format="color" />
        <attr name="button_normal_stroke_color" format="color" />
        <attr name="text_normal_color" format="color" />
        <attr name="button_normal_stroke_width" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BGASwipeItemLayout">
        <attr name="bga_sil_swipeDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="bga_sil_bottomMode" format="enum">
            <enum name="pullOut" value="0" />
            <enum name="layDown" value="1" />
        </attr>
        <attr name="bga_sil_springDistance" format="dimension" />
        <attr name="bga_sil_swipeAble" format="boolean" />
    </declare-styleable>
</resources>
